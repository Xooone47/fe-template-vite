{
  "name": "fe-template-vite",
  "version": "0.0.1",
  "description": "To build a react app quickly",
  "scripts": {
    "start": "vite",
    "clean": "rm -rf dist/",
    "build": "vite build",
    "build:preview": "vite build --mode preview",
    "test": "jest --config ./configs/jest.config.js",
    "test:watch": "jest --config ./configs/jest.config.js --watch",
    "preview": "vite preview",
    "lint": "pnpm lint-es && pnpm lint-type",
    "lint-es": "eslint src --ext .tsx,.ts,.js,.jsx -c .eslintrc.js --ignore-path ./configs/.eslintignore",
    "lint-type": "tsc",
    "analyze": "vite build --report",
    "preinstall": "npx only-allow pnpm"
  },
  "bin": {
    "fe": "bin/index.js"
  },
  "babel": {
    "extends": "./configs/babel.config.js"
  },
  "author": "Dylan Li",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-vite": "^1.3.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.2",
    "ts-jest": "^29.1.2",
    "vite": "^5.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.15.0",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "commander": "^12.0.0",
    "copy-to-clipboard": "^3.3.3",
    "core-js": "^3.36.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "recoil": "^0.7.7",
    "typescript": "^5.4.3",
    "use-immer": "^0.11.0",
    "zustand": "^4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm lint && pnpm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -c .eslintrc.js --ignore-path ./configs/.eslintignore --fix"
    ]
  },
  "engines": {
    "node": ">= 16.16.0",
    "pnpm": "^9.0.0"
  }
}
